{
  "name": "@ducdev2k1/file-icon",
  "description": "A Vue 3 component library for file icons",
  "version": "0.0.8",
  "type": "module",
  "main": "./dist/file-icon.umd.js",
  "module": "./dist/file-icon.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "sideEffects": [
    "*.svg",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.gif"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ducdev2k1/image-icon-file.git"
  },
  "bugs": {
    "url": "https://github.com/ducdev2k1/image-icon-file/issues"
  },
  "homepage": "https://github.com/ducdev2k1/image-icon-file#readme",
  "author": "ducdev2k1",
  "keywords": [
    "vue",
    "icon",
    "file",
    "file-icon",
    "library",
    "icon file office"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --mode lib",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "mime": "^4.0.6"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "rollup-plugin-dts": "^6.2.1",
    "vite-plugin-dts": "^2.0.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  }
}
